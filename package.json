    "postcss-font-base64": "^1.0.5",
    "postcss-modules": "^1.4.1",
    "pulp": "^13.0.0",
    "purescript": "^0.13.0",
    "purescript-psa": "^0.7.3",
    "rimraf": "^2.6.3"
